{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "tasks-manager/tasks-progress",
    "title": "Tasks Manager Block",
    "icon": "calendar",
    "category": "layout",
    "attributes": {
        "wrapperBackgroundColor": {
            "type": "string",
            "default": ""
        },
        "groupSpacing": {
            "type": "object",
            "default": {}
        },
        "wrapperBorder": {
            "type": "object",
            "default": {}
        },
        "wrapperPadding": {
            "type": "object",
            "default": {}
        },
        "listBorder": {
            "type": "object",
            "default": {}
        },
        "listCardSpacing": {
            "type": "object",
            "default": {
                "all": "0px"
            }
        },
        "listPadding": {
            "type": "object",
            "default": {}
        },
        "listBackgroundColor": {
            "type": "string",
            "default": ""
        },
        "listTextColor": {
            "type": "string",
            "default": ""
        },
        "listTextSize": {
            "type": "string",
            "default": ""
        },
        "cardTextSize": {
            "type": "string",
            "default": ""
        },
        "cardTextColor": {
            "type": "string",
            "default": ""
        },
        "gapSize": {
            "type": "string",
            "default": ""
        },
        "cardBorder": {
            "type": "object",
            "default": {}
        },
        "cardPadding": {
            "type": "object",
            "default": {}
        },
        "cardBackgroundColor": {
            "type": "string",
            "default": ""
        }
    },
    "supports": {
        "align": [
            "wide",
            "full"
        ],
        "anchor": true,
        "html": true
    },
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}